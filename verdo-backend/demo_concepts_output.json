        {
          "id": "elem_003",
          "type": "paragraph",
          "content": "An externality occurs when a person engages in an activity that influences the well-being of a bystander and yet neither pays nor receives any compensation for that effect.",
          "page": 1,
          "confidence": 0.88,
          "position_in_page": 3,
          "global_sequence": 3
        },
        {
          "id": "elem_004",
          "type": "paragraph",
          "content": "If the impact on the bystander is adverse, it is called a negative externality. If it is beneficial, it is called a positive externality.",
          "page": 1,
          "confidence": 0.9,
          "position_in_page": 4,
          "global_sequence": 4
        }
      ]
